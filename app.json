{
  "expo": {
    "name": "HYBRID",
    "slug": "hybrid",
    "scheme": "hybridworkout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a141f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaredwaldroff.hybrid",
      "buildNumber": "2",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "HYBRID needs access to your photo library to upload photos to your posts and profile.",
        "NSCameraUsageDescription": "HYBRID needs access to your camera to take photos for your posts.",
        "NSCalendarsUsageDescription": "HYBRID needs access to your calendar to sync your workouts.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.jaredwaldroff.hybrid",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a141f"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "HYBRID needs access to your photo library to upload photos to your posts and profile."
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "HYBRID needs access to your calendar to sync your workouts."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ae4e3783-a643-4a7e-95d4-eb3108c0184a"
      }
    }
  }
}